    "def diagonalDifference(arr):\n",
    "    left = 0\n",
    "    right = 0\n",
    "    ln = len(arr)\n",
    "    for i in range(ln):\n",
    "        left += arr[i][i]\n",
    "        right += arr[i][ln - i - 1]\n",
    "    \n",
    "    return(abs(left - right))"
   ]
