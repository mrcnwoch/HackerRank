   "source": [
    "def pageCount(n, p):\n",
    "    if n % 2 == 0:\n",
    "        n += 1\n",
    "    \n",
    "    left_flips = p // 2\n",
    "    right_flips = (n - p) // 2\n",
    "    \n",
    "    return(min(left_flips, right_flips))"
   ]
  },
