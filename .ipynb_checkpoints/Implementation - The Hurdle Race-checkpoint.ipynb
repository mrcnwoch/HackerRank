   "source": [
    "def hurdleRace(k, height):\n",
    "    magic_potion = 0\n",
    "    \n",
    "    for h in height:\n",
    "        if k < h:\n",
    "            diff = h - k\n",
    "            if magic_potion < diff:\n",
    "                magic_potion = diff\n",
    "    \n",
    "    return magic_potion"
   ]
  },
