   "source": [
    "def catAndMouse(x, y, z):\n",
    "    diffA = abs(x-z)\n",
    "    diffB = abs(y-z)\n",
    "    \n",
    "    if diffA == diffB:\n",
    "        return 'Mouse C'\n",
    "    elif diffA < diffB:\n",
    "        return 'Cat A'\n",
    "    else:\n",
    "        return 'Cat B'"
   ]
