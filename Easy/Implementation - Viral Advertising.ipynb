   "source": [
    "def viralAdvertising(n):\n",
    "    shared = 5\n",
    "    liked = 0\n",
    "    cumulative = 0\n",
    "    \n",
    "    for i in range(n):\n",
    "        liked = shared // 2\n",
    "        cumulative += liked\n",
    "        shared = liked * 3\n",
    "        \n",
    "    return cumulative"
   ]
