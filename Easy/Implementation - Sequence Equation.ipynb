   "source": [
    "def permutationEquation(p):\n",
    "    ret = []\n",
    "    for i in range(1, len(p) + 1):\n",
    "        idx1 = p.index(i) + 1\n",
    "        idx2 = p.index(idx1) + 1\n",
    "        ret.append(idx2)\n",
    "        \n",
    "    return ret"
   ]
