    "def maximumToys(prices, k):\n",
    "    prices.sort()\n",
    "    \n",
    "    total = 0\n",
    "    cnt = 0\n",
    "    for price in prices:\n",
    "        if total + price <= k:\n",
    "            total += price\n",
    "            cnt += 1\n",
    "            \n",
    "    return cnt"
   ]
