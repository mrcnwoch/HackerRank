   "source": [
    "def fibonacci(n):\n",
    "    fib0 = 0\n",
    "    fib1 = 1;\n",
    "    \n",
    "    if n == 0:\n",
    "        return 0\n",
    "    elif n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        for i in range(2, n + 1):\n",
    "            tmp = fib0\n",
    "            fib0 = fib1\n",
    "            fib1 = tmp + fib1\n",
    "\n",
    "    return fib1"
   ]
